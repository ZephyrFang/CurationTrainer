{
  "expo": {
    "name": "Curation Trainer",
    "slug": "CurationTrainer",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist":{
        "NSPhotoLibraryUsageDescription": "Give Expo experiences permission to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Give Expo experiences perimssion to save photos."
      }
    }
  }
}