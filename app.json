{
  "expo": {
    "name": "Curation Trainer",
    "description": "Data Training for Curation App",
    "slug": "CurationTrainer",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/HappyMooseLogo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "nz.happymoose.curationtrainer",
      "supportsTablet": true,
      "infoPlist":{
        "NSPhotoLibraryUsageDescription": "Users need to submit photos from their Photo Library to this app."        
      }      
    },
    "android": {
      "package": "nz.happymoose.curationtrainer"
    }
  }
}